        {
          "name": "Hero",
          "element": "section",
          "className": "min-h-[60vh] pt-8 pb-12 flex items-center bg-gradient-to-br from-background to-accent/20",
          "container": "container mx-auto px-4",
          "layout": "grid grid-cols-1 lg:grid-cols-12 gap-6 items-center",
          "motionVariants": {
            "initial": { "opacity": 0, "y": 20 },
            "animate": { "opacity": 1, "y": 0 },
            "transition": { "duration": 0.8 }
          },
          "children": [
            {
              "name": "Content",
              "className": "lg:col-span-7 space-y-6",
              "elements": [
                {
                  "name": "AnimatedLogo", 
                  "className": "mb-6",
                  "responsive": "hidden on mobile (!isMobileDevice)"
                },
                {
                  "name": "Title",
                  "element": "h1",
                  "className": "text-3xl lg:text-5xl font-bold",
                  "motionDelay": 0.3
                },
                {
                  "name": "RotatingSubtitle",
                  "element": "h2", 
                  "className": "text-xl lg:text-2xl text-muted-foreground",
                  "motionDelay": 0.6
                },
                {
                  "name": "Description",
                  "element": "p",
                  "className": "text-lg text-muted-foreground leading-relaxed max-w-2xl",
                  "motionDelay": 0.9
                },
                {
                  "name": "CTA Button",
